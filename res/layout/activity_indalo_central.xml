<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background_building_inside"
    tools:context=".IndaloCentralActivity">

    <include
        android:id="@+id/include2"
        layout="@layout/toolbar_top"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <include
        android:id="@+id/include"
        layout="@layout/toolbar_bottom"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.65" />

    <ImageView
        android:layout_width="408dp"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/include"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/guideline10"
        app:layout_constraintTop_toBottomOf="@+id/include2"
        app:srcCompat="@drawable/product_indalo_central" />

    <ImageButton
        android:id="@+id/ibHealth"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:background="@color/transparent"
        android:onClick="ibHealthClicked"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toTopOf="@+id/ibTechnology"
        app:layout_constraintEnd_toStartOf="@+id/guideline10"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/ibDemostration"
        app:layout_constraintTop_toBottomOf="@+id/include2"
        app:srcCompat="@drawable/icon_indalo_central_health" />

    <ImageButton
        android:id="@+id/ibDemostration"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="8dp"
        android:background="@color/transparent"
        android:onClick="ibDemostrationClicked"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toTopOf="@+id/ibEconomy"
        app:layout_constraintEnd_toStartOf="@+id/ibHealth"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/include2"
        app:srcCompat="@drawable/icon_indalo_central_demostration" />

    <ImageButton
        android:id="@+id/ibEconomy"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/icon_indalo_rob_economy"
        android:onClick="ibEconomyClicked"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toTopOf="@+id/include"
        app:layout_constraintEnd_toStartOf="@+id/ibTechnology"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ibDemostration"
        app:srcCompat="@drawable/icon_indalo_rob_economy" />

    <ImageButton
        android:id="@+id/ibTechnology"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_marginEnd="156dp"
        android:layout_marginBottom="8dp"
        android:background="@color/transparent"
        android:onClick="ibTechnologyClicked"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toTopOf="@+id/include"
        app:layout_constraintEnd_toStartOf="@+id/guideline10"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/ibEconomy"
        app:layout_constraintTop_toBottomOf="@+id/ibHealth"
        app:srcCompat="@drawable/icon_indalo_central_technology" />

    <ImageButton
        android:id="@+id/ibVideo"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_marginStart="20dp"
        android:background="@color/transparent"
        android:onClick="ibVideoClicked"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="@+id/ibTechnology"
        app:layout_constraintStart_toEndOf="@+id/ibTechnology"
        app:layout_constraintTop_toTopOf="@+id/ibTechnology"
        app:srcCompat="@drawable/icon_indalo_rob_video" />

</androidx.constraintlayout.widget.ConstraintLayout>